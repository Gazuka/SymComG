{# require fichier #}
<tr>
    <td>
        {{ fichier.media.document.id }}
    </td>
    <td class='col-2'>
        <a href="{{ path('admin_medias_document', {'iddocument': fichier.media.document.id}) }}">
            {{ fichier.nom }}
        </a>
    </td>
    <td class='col-3'>
        <a href="{{ path('admin_medias_document', {'iddocument': fichier.media.document.id}) }}">
            {% include 'entity/classeur/image/_voir____media____image.html.twig' with {'media': fichier.media, 'liipFiltre': 'icone6' } only %}            
        </a>
    </td>
    <td>
        <a href="{{ path('admin_medias_document', {'iddocument': fichier.media.document.id}) }}">
            {{ fichier.media.image.alt }}
        </a>
    </td>
</tr>