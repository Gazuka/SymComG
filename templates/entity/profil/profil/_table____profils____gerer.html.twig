{#
    require profils
#}
{% for profil in profils %}
    <tr>
        <td>
            {{ profil.id }}
        </td>
        <td>
            <a href="{{ path('admin_profil_gerer', {'idprofil': profil.id}) }}">{{ profil.humain.nom }} {{ profil.humain.prenom }}</a>
        </td>
        <td>
            {{ profil.hierarchie }}
        </td>
    </tr>
{% endfor %}